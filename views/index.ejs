<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" href="bower_components/normalize-css/normalize.css">

  <script type="text/javascript">
    var apiKey = '<%= apiKey %>';
    var sessionId = '<%= sessionId %>';
    var token = '<%= token %>';
    var userNameList = '<%= userNameList %>';
    console.log(userNameList);
  </script>

  <script src="https://cdn.firebase.com/js/client/1.0.18/firebase.js"></script>
  <script src="//static.opentok.com/webrtc/v2.2/js/opentok.min.js"></script>
  <link rel="stylesheet" href="css/style.css">
  <title>ParaSite Club</title>
</head>

<body>

<!--Login Div -->

  <div class="login page">
      <h1 class="title">Your nickname?</h1>
      <input class="userNameInput" type="text" maxlength="14" autofocus >
      <h3 class="usersUpdate">There are currently <span class="usersNum"><%= numUsers %></span> users</h3>

  </div>

<!-- Main Chat Div  -->
    <div class="chat page">

      <section class="chatArea">
        <ul class="messages"></ul>
      </section>

<!-- Videos Div  -->

  <aside class="videoArea">
    <div id="serverVidBox"><meter id="meter-1" min=0 max=50 value=0></meter></div>

    <div class="userVideoBox">
      <div id="user0" class="vidBox"><meter id="meter0" min=0 max=50 value=0></meter></div>
      <div id="user1" class="vidBox"><meter id="meter1" min=0 max=50 value=0></meter></div>
      <div id="user2" class="vidBox"><meter id="meter2" min=0 max=50 value=0></meter></div>
      <div id="user3" class="vidBox"><meter id="meter3" min=0 max=50 value=0></meter></div>
      <div id="user4" class="vidBox"><meter id="meter4" min=0 max=50 value=0></meter></div>
      <div id="user5" class="vidBox"><meter id="meter5" min=0 max=50 value=0></meter></div>
      <div id="user6" class="vidBox"><meter id="meter6" min=0 max=50 value=0></meter></div>
      <div id="user7" class="vidBox"><meter id="meter7" min=0 max=50 value=0></meter></div>
      <div id="user8" class="vidBox"><meter id="meter8" min=0 max=50 value=0></meter></div>
    </div>

  </aside>

<!-- Text Input Box   -->

  <footer class="messagePost">
      <input class="inputMessage" placeholder="Type here..." autofocus >
  </footer>


<!-- End Chat Div  -->
  </div>

  <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
  <script src="/socket.io/socket.io.js"></script>
  <script src="js/main.js"></script>


</body>

</html>
